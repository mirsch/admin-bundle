{
    "name": "mirsch/admin-bundle",
    "description": "Symfony Admin Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "mirsch"
        }
    ],
    "require": {
        "php": ">=5.6",
        "adamquaile/symfony-fieldset-bundle": "dev-master",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/translation-bundle": "^1.3",
        "ju1ius/webpack-assets-bundle": "^0.1.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sg/datatablesbundle": "^0.13.0",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/symfony": "3.2.*",
        "tetranz/select2entity-bundle": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Mirsch\\Bundle\\AdminBundle\\": ""
        }
    },
    "require-dev": {
        "spryker/code-sniffer": "^0.8.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "cs-check": "vendor/bin/phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml --ignore=admin-bundle/vendor --exclude=Spryker.Namespaces.UnusedUseStatement ./",
        "cs-fix": "vendor/bin/phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml --ignore=admin-bundle/vendor --exclude=Spryker.Namespaces.UnusedUseStatement ./"
    }
}
